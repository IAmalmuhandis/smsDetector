{
  "expo": {
     "name": "SMSDETECTOR",
    "plugins": [
      [
        "expo-camera",
       

        {
          "cameraPermission": "Allow $(scannerapp) to access your camera."
        }
      ],
      [
        "expo-barcode-scanner",
        {"cameraPermission" :"Allow $(scannerapp) to access your camera."}
      ]
    ],
    "slug": "basicapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "READ_SMS",
        "RECEIVE_SMS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
